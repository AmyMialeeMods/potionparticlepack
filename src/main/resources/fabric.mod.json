{
  "schemaVersion": 1,
  "id": "potionparticlepack",
  "version": "${version}",
  "name": "potionparticlepack",
  "description": "Pretty Potion Particles",
  "authors": [
    "AmyMialee"
  ],
  "contact": {
    "repo": "https://github.com/AmyMialeeMods/potionparticlepack"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/potionparticlepack/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "xyz.amymialee.potionparticlepack.PotionParticlePackDataGen"
    ],
    "preLaunch": [
      "xyz.amymialee.potionparticlepack.PotionParticlePackPreLaunch"
    ],
    "client": [
      "xyz.amymialee.potionparticlepack.PotionParticlePackClient"
    ],
    "main": [
      "xyz.amymialee.potionparticlepack.PotionParticlePack"
    ],
    "cardinal-components-entity": [
      "xyz.amymialee.potionparticlepack.PotionParticlePackComponents"
    ]
  },
  "mixins": [
    "potionparticlepack.mixins.json"
  ],
  "accessWidener": "potionparticlepack.accesswidener",
  "custom": {
    "cardinal-components": [
      "potionparticlepack:status"
    ]
  },
  "depends": {
    "fabric": "*"
  }
}